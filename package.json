{
  "name": "syncollege",
  "version": "0.0.1",
  "description": "syncollege",
  "readme": "deployment.md",
  "scripts": {
    "itaobao": "npm install --registry=https://registry.npm.taobao.org/",
    "wp-dev": "source bashrc.local; cd web; rm -r web/build; mkdir -p build; node --harmony ./wp.js --dev",
    "wp-build": "source bashrc.local; cd web;rm -r build; mkdir -p build; node --harmony ./wp.js --prod --build",
    "start-web-dev": "source bashrc.local; cd web/server; node --harmony ./server.js --dev",
    "start-web-prod": "cd web/server; node --harmony ./server.js --prod",
    "start-api-dev": "source bashrc.local; cd api; gulp debug --harmony --dev",
    "start-api-prod": "cd api; node --harmony ./server.js --prod",
    "test-model": "source bashrc.local; cd api && gulp mocha-model",
    "test-route": "source bashrc.local; cd api && gulp mocha-route",
    "push": "npm run wp-build; git add .;git commit -m 'ready to publish';git push",
    "run-web": "docker rm -f web;docker build -t web:0.0.1 -f Dockerfile.web .;docker run -d -p 8000:8000 -v /root/source:/etc/source --name web web:0.0.1;docker logs -f web",
    "run-api": "docker rm -f api;docker build -t api:0.0.1 -f Dockerfile.api .;docker run -d -p 3000:3000 -v /root/source:/etc/source --name api api:0.0.1;docker logs -f api"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccnuyan/syncollege.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "ccnuyan <ccnuyan@live.com> (http://github.com/ccnuyan)",
  "license": "MIT",
  "homepage": "https://github.com/ccnuyan/syncollege",
